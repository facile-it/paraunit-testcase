{
  "name": "facile-it/paraunit-testcase",
  "type": "library",
  "description": "paraunit testcase",
  "keywords": [
    "parallel test",
    "test case",
    "symfony",
    "doctrine",
    "isolation",
    "transaction",
    "phpunit"
  ],
  "homepage": "http://github.com/facile-it/paraunit-testcase",
  "license": "Apache License Version 2.0",
  "authors": [
    {
      "name": "Francesco Panina",
      "email": "francesco.panina@gmail.com"
    },
    {
      "name": "Alessandro Lai",
      "email": "alessandro.lai85@gmail.com"
    }
  ],
  "require": {
    "doctrine/orm": ">=2.2.3,<2.6-dev",
    "liip/functional-test-bundle": "~1.3",
    "symfony/config": "~2.3|~3.0",
    "symfony/browser-kit": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "ParaunitTestCase\\": "src/ParaunitTestCase"
    }
  },
  "minimum-stability": "dev",
  "suggest": {
    "facile-it/paraunit": "For parallel test running; it's faster!"
  }
}
